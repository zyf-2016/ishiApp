.detail-result-title {
    text-align: center;
    line-height: 50px;
    color: #333;
    padding: 30px 0;
}

.detail-result-title image {
    display: block;
    margin: 0 auto;
    width: 50px;
    height: 50px;
}

.detail-result-block {
    padding: 0 30px;
}

.detail-result-text {
    color: #505050;
    background: #f5f5f5;
    border-radius: 4px;
    padding: 0 15px;
    font-size: 14px;
    line-height: 42px;
}

.detail-result-text .text-block {
    border-bottom: 1px solid #e7e7e7;
}

.detail-result-text .text-block.no-line {
    border: 0;
}

.detail-result-text .text-block text {
    float: right;
    color: #333;
}

.detail-result-btn-block {
    padding: 30px 0 0 0;
}

.detail-result-btn-block .btn-block {
    width: 50%;
    float: left;
    padding: 0 8px 0 0;
    box-sizing: border-box;
}

.bjj { width: 100%; float: left; background-image: url(https://api.shi1.cn/open/oss/dt.png); background-size: 100%; background-position:center center; background-repeat:no-repeat; height: 100%; position: absolute;}
.zql { width: 100%; font-size: 24px; color: #c02221; text-align: center; line-height: 40px; float: left; }
